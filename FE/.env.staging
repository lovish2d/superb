# Staging Environment Configuration

# API Configuration
VITE_API_BASE_URL=https://api-staging.superb.ai
VITE_API_TIMEOUT=30000

# Authentication
VITE_AUTH_TOKEN_KEY=superb_auth_token_staging
VITE_AUTH_REFRESH_TOKEN_KEY=superb_refresh_token_staging

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_DEBUG_MODE=false

# Application Configuration
VITE_APP_NAME=Superb Aviation (STAGING)
VITE_APP_VERSION=1.0.0-staging

# External Services
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/staging
VITE_GOOGLE_ANALYTICS_ID=

# Environment
VITE_ENVIRONMENT=staging
