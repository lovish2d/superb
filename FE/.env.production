# Production Environment Configuration

# API Configuration
VITE_API_BASE_URL=https://api.superb.ai
VITE_API_TIMEOUT=30000

# Authentication
VITE_AUTH_TOKEN_KEY=superb_auth_token
VITE_AUTH_REFRESH_TOKEN_KEY=superb_refresh_token

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_DEBUG_MODE=false

# Application Configuration
VITE_APP_NAME=Superb Aviation
VITE_APP_VERSION=1.0.0

# External Services
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/production
VITE_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X

# Environment
VITE_ENVIRONMENT=production
