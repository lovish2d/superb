# Pre-Production Environment Configuration

# API Configuration
VITE_API_BASE_URL=https://api-preprod.superb.ai
VITE_API_TIMEOUT=30000

# Authentication
VITE_AUTH_TOKEN_KEY=superb_auth_token_preprod
VITE_AUTH_REFRESH_TOKEN_KEY=superb_refresh_token_preprod

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_DEBUG_MODE=false

# Application Configuration
VITE_APP_NAME=Superb Aviation (PRE-PROD)
VITE_APP_VERSION=1.0.0-preprod

# External Services
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/preprod
VITE_GOOGLE_ANALYTICS_ID=

# Environment
VITE_ENVIRONMENT=preprod
